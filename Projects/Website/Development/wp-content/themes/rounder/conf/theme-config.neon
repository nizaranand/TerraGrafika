							},
							title: {
								label: Title,
								type: text,
								default: 'Facebook'
							},
							link: {
								label: Link,
								type: text,
								default: 'http://www.facebook.com/AitThemes'
							},
						],
						[
							iconUrl: {
								label: Icon URL,
								type: image-url,
								default: 'design/img/ico/yt.png'
							},
							title: {
								label: Title,
								type: text,
								default: Whatch us on Youtube.
							},
							link: {
								label: Link,
								type: text,
								default: 'http://www.youtube.com/user/AitThemes'
							},
						],
						[
							iconUrl: {
								label: Icon URL,
								type: image-url,
								default: 'design/img/ico/tw.png'
							},
							title: {
								label: Title,
								type: text,
								default: Follow us on Twitter
							},
							link: {
								label: Link,
								type: text,
								default: 'http://www.twitter.com/AitThemes'
							},
						],
						[
							iconUrl: {
								label: Icon URL,
								type: image-url,
								default: 'design/img/ico/fl.png'
							},
							title: {
								label: Title,
								type: text,
								default: Flickr...
							},
							link: {
								label: Link,
								type: text,
								default: 'http://www.flickr.com'
							},
						],
						[
							iconUrl: {
								label: Icon URL,
								type: image-url,
