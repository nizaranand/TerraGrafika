appearance:
	title:     Appearance Settings
	menu-title: Appearance
	tabs:
		colors:
			title: Colors & Bacgkrounds
			tab-title: Colors & Bacgkrounds
			less: true
			options:
				Website Background: section
				#################
				htmlBgColor:
					label: Background Color
					type: colorpicker
					default: '#f9f9f9'
					themebox: { label: Background, page: all }
				htmlBg:
					label: Background Image
					type: image-url
					default: ''					
				htmlBgY:
					label: Vertical Position
					type: select
					default: {
						top: {
							label: Top
							checked: true
						},
						center: {
							label: Center
						},
						bottom: {
							label: Bottom
						}
					}
				htmlBgX:
					label: Horizontal Position
					type: select
					default: {
						left: {
							label: Left
						},
						center: {
							label: Center
							checked: true
						},
						right: {
							label: Right
						}
					}
				htmlBgRepeat:
					label: Repeat
					type: select
					default: {
						no-repeat: {
							label: No Repeat
						},
						repeat: {
							label: Repeat
						},
						repeat-x: {
							label: Repeat X
							checked: true
						},
						repeat-y: {
							label: Repeat Y
						}
					}
				htmlBgAttach:
					label: Scroll the background?
					type: radio
					themebox: {page: all}
					default:
						scroll:
							label: 'Yes'
							checked: true
						fixed:
							label: 'No'
				topPanelBgColor:
					label: Top Panel Background Color
					type: colorpicker
					default: '#333333'
					themebox: { label: TopStrip Bg, page: all }
				headerBgColor:
					label: Header Background Color
					type: colorpicker
					default: '#f9f9f9'
					themebox: { label: Header Bg, page: all }
				footerBgColor:
					label: Background Color
					type: colorpicker
					default: '#333333'
					themebox: { label: TopStrip Bg, page: all }
				Website Decoration: section
				#################
				decorColor:
					label: Decorative Color
					type: colorpicker
					default: "#3ed4e5"	
					themebox: { label: Theme, page: all }
				secondDecorColor:
					label: Decorative Color
					type: colorpicker
					default: "#92da4d"	
					themebox: { label: Theme, page: all }
				linesColor:
					label: Lines Color
					type: colorpicker
					default: '#e3e3e3'
					themebox: { label: Lines, page: all }
		fonts:
			title:     Fonts
			tab-title: Fonts
			less: true
			options:
				Font colors: section
				#################
				generalFontColor:
					label:  General Text Color
					type: colorpicker
					default: '#777777'
					themebox: {label: General Font Color, page: all}
				menusFontColor:
					label: Menu Font Color
					type: colorpicker
					default: '#ffffff'
					themebox: {label: Menu Font, page: all}
				titlesFontColor:
					label:  Titles Font Color
					type: colorpicker
					default: '#333333'
					themebox: {label: Titles Font, page: all}
				linksFontColor:
					label: Links Font Color
					type: colorpicker
					default: '#3ed4e5'
					themebox: {label: Links Font, page: all}
				footerTextFontColor:
					label: Footer Text Color
					type: colorpicker
					default: '#879ea9'
					themebox: {label: Footer Text, page: all}
				footerTitlesFontColor:
					label: Footer Titles Color
					type: colorpicker
					default: '#ffffff'
					themebox: {label: Footer Titles, page: all}
				footerDecorFontColor:
					label:  Footer Decorations Font Color
					type: colorpicker
					default: '#3ed4e5'
					themebox: {label: Footer Decor, page: all}
				footerLinksFontColor:
					label: Footer Links Color
					type: colorpicker
					default: '#879ea9'
					themebox: {label: Footer Links, page: all}
		customCss:
			title: Custom CSS
			tab-title: Custom CSS
			options:
				Your custom CSS: section
				########################
				customCss:
					type: custom-css
					default: '/* your custom CSS in LESS syntax */'
				LESS variables for images: section
				########################
				customImages:
					type: custom-css-vars
					default: [
						[
							variable: {
								label: Image name
								type: text
							},
							value: {
								label: Image
								type: image-url
							}
						]

					]
