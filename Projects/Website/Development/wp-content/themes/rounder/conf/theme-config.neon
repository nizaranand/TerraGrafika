generalOptions:
	title: General Settings
	menu-title: General Settings
	tabs:
		general:
			title:     General Setting
			tab-title: General
			options:
				Top-Header: section
				#################
				logo_img:
					label: Logo image
					type: image-url
					default: 'design/img/logo.png'
					skinable: false
				tagline:
					label: Tagline
					type: text
					default: 'abstract design<br>for website presentation'
				Main Menu: section
				#################
				mainmenu_dropdown_time:
					label: "Dropdown Animation Time (ms)"
					type: text
					default: 0.4
				mainmenu_dropdown_animation:
					label: "Dropdown Animation Easing"
					type: select
					default: {
						linear: {
							label: linear
						},
						ease: {
							label: ease
						},
						ease-in: {
							label: ease-in
						},
						ease-out: {
							label: ease-out
						},
						ease-in-out: {
							label: ease-in-out
							checked: true
						}
					}
				Comments: section
				#################
				closeComments:
					label: Closeable comments
					type: checkbox
					default:
						enable:
							label: 'Yes'
							checked: true
				defaultPosition:
					label: "Comments position"
					type: select
					default:
						opened:
							label: "Opened"
						closed:
							label: "Closed"
							checked: true
				CSS3 Features: section
				#################
				enableCSSFeatures:
					label: Enable Features
					type: checkbox
					default:
						enable:
							label: 'Yes'
							checked: true
				Footer: section
				#################
				footer_text:
					label: Footer text
					type: wysiwyg
					default: '<a href="http://www.ait-themes.com" style="color: #fff">&copy; 2012 Copyright by AIT.</a> All rights reserved. Lorem ipsum dolor sit amet consecteur...'
					help: 'This text will be displayed in the footer of page'
				Google Codes: section
				#################
				ga_code:
					label: Google Analytics Tracking Code
					type: text
		globals:
			title: Global settings
			tab-title: Sections
			options:
				Header: section
				#######################
				headerIcon:
					label: Header Icon
					type: image-url
					default: 'design/img/ico1.png'
				Global sections order: section
				#######################
				sectionsOrder:
					type: order
					meta: '_ait_sections_order'
					type: order
					default:
						sectionA: Section A - Content
						sectionB: Section B - Testimonials
						sectionC: Section C - Service box
						sectionD: Section D - Presentation
				Slider - Global Settings : section
				#################
				sliderEnable:
					label: Show
					type: radio
					default:
						true:
							label: 'Yes'							
							checked: true
						false:
							label: 'No'
				sliderType:
					label: Slider type
					type: dropdown-sliders
					default: Revolution slider
				sliderCategory:
					label: Anything slider
					type: dropdown-categories
					default: slider-creator
				sliderAliases:
					label: Revolution slider
					type: dropdown-slider-aliases
					default: subSlider
				sliderAlternative:
					label: Slider alternative
					type: image-url
					default: ''
				sliderHeight:
					label: "Slider Height (px)"
					type: text
					default: 442
				sliderBgImg:
					label: "Slider Background Image"
					type: image-url
					default: 'http://testing.ait-themes.com/rounder/wp1/wp-content/themes/rounder/design/img/subpage-slider-bg.png'
				Service boxes - Global Settings : section
				#################
				showServiceBoxes:
					label: Show
					type: radio
					default: {
						'yes': {
							label: 'Yes'
							checked: true
						},
						'no': {
							label: 'No'
						}
					}
				globalServiceBoxes:
					label: Service Boxes Category
					type: dropdown-categories
					default: 'services'
				Presentation - Global Settings : section
				#################
				showPresentation:
					label: Show
					type: radio
					default:
						true:
							label: 'Yes'
						false:
							label: 'No'
							checked: true
				globalPresentation:
					label: Show Presentation Category
					type: dropdown-categories
					default: 'presentations'
				presentationTitle:
					label: Title
					type: text
					default: Lorem ipsum dolor sit amet consectetur elit
				presentationEffect:
					label: Effect
					type: select
					default: 
						'Random':
							label: Random
						'FadeIn':
							label: FadeIn
							checked: true
						'ScrollIn':
							label: ScrollIn
						'ScrollOut':
							label: ScrollOut
				Testimonials - Global Settings : section
				#################
				showTestimonials:
					label: Show
					type: radio
					default:
						true:
							label: 'Yes'
						false:
							label: 'No'
							checked: true
				testimonialsText:
					label: Testimonials Text
					type: textarea
				testimonialsBgColor:
					label: Background Color
					type: colorpicker
					default: "#f9f9f9"
				testimonialsColor:
					label: Text Color
					type: colorpicker
					default: "#333333"
				testimonialsLinkColor:
					label: Testimonial Link Color
					type: colorpicker
					default: "#3ed4e5"
		footer:
			title:     Footer Settings
			tab-title: Footer Widgets
			less: true
			options:
				Footer - Widgets : section
				#################
				widthFirst:
					label: "First Widget Width"
					type: text
					skinable: true
					default: 307px
				widthSecond:
					label: "Second Widget Width"
					type: text
					skinable: true
					default: 180px
				widthThird:
					label: "Third Widget Width"
					type: text
					skinable: true
					default: 180px
				widthFourth:
					label: "Fourth Widget Width"
					type: text
					skinable: true
					default: 201px
				widthFifth:
					label: "Fifth Widget Width"
					type: text
					skinable: true
					default: 202px
				widthSixth:
					label: "Sixth Widget Width"
					type: text
					skinable: true
					default: 202px
		socialIcons:
			title: Social icons
			tab-title: Social icons
			options:
				"Icons": section-title
				displayIcons:
